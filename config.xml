<?xml version='1.0' encoding='utf-8'?>
<widget
 xmlns:gap = "http://phonegap.com/ns/1.0"
 id="qmacro.iconfinder"
 version="0.1"
 xmlns="http://www.w3.org/ns/widgets">
 <name>UI5 Icon Finder</name>
 <description>
   Find Icons in UI5
 </description>
 <author email="dj.adams@pobox.com" href="http://pipetree.com/qmacro/">
   DJ Adams
 </author>
 <content src="index.html" />
 <access origin="*" />

 <!-- Camera Plugin , stopping geolocation currently -->
 <gap:plugin name="com.sharinglabs.cordova.plugin.cache" version="1.0.0" />

 <gap:plugin name="com.phonegap.plugins.barcodescanner"/>

 <!-- <gap:plugin name="org.apache.cordova.device-motion" version="0.1.0" />

 <gap:plugin name="org.apache.cordova.geolocation" version="0.3.2" /> -->

 <gap:config-file platform="ios" parent="UIViewControllerBasedStatusBarAppearance" overwrite="true"> <false/>
 </gap:config-file>

 <gap:config-file platform="ios" parent="UIStatusBarHidden" overwrite="true"> <true/> </gap:config-file>

 <feature name="Accelerometer">
   <param name="android-package" value="org.apache.cordova.devicemotion.AccelListener" />
 </feature>

 <feature name="http://api.phonegap.com/1.0/geolocation"/>


</widget>
